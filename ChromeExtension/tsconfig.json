{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "suppressImplicitAnyIndexErrors": true,
        "inlineSourceMap": true,
        "allowJs": true,
        "rootDir": ".",
        "outDir": "dist/",
        "noEmitOnError": true
    },
    "exclude": [
        "node_modules",
        "**/node_modules/*",
        "webpack.*"
    ]
}